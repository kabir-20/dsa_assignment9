#include <bits/stdc++.h>
using namespace std;

char grid[100][100];
int vis[100][100];
int M, N;

int dr[4] = {1,-1,0,0};
int dc[4] = {0,0,1,-1};

void dfs(int r, int c) {
    vis[r][c] = 1;
    for (int k = 0; k < 4; k++) {
        int nr = r + dr[k], nc = c + dc[k];
        if (nr >= 0 && nr < M && nc >= 0 && nc < N)
            if (!vis[nr][nc] && grid[nr][nc] == '1')
                dfs(nr, nc);
    }
}

int main() {
    cin >> M >> N;

    for (int i = 0; i < M; i++)
        for (int j = 0; j < N; j++)
            cin >> grid[i][j];

    int cnt = 0;

    for (int i = 0; i < M; i++)
        for (int j = 0; j < N; j++)
            if (!vis[i][j] && grid[i][j] == '1') {
                dfs(i, j);
                cnt++;
            }

    cout << cnt;
}
